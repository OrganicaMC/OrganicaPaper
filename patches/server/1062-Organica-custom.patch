+            var vec = entity.position().vectorTo(player.position()).normalize();
+            vec = new Vec3(vec.x, 0, vec.z);
+            if (vec.dot(player.getViewVector(1f)) >= 0) continue;
+            entity.setSecondsOnFire(6);
