+
+/**
+ * Used to mark general polymer objects
+ */
+public interface PolymerObject {
+    Predicate<Object> PREDICATE = (obj) -> obj instanceof PolymerObject;
+    Predicate<Object> PREDICATE_NOT = (obj) -> !(obj instanceof PolymerObject);
