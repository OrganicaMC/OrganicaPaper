+        return EventFactory.createArrayBacked(ItemGroupEvents.ModifyEntries.class, callbacks -> (entries) -> {
+            for (ItemGroupEvents.ModifyEntries callback : callbacks) {
+                callback.modifyEntries(entries);
+            }
+        });
+    }
+}
